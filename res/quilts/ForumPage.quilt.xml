    <component id="quilt:Footer"/>
  </add>
  <add to="side-content">
    <component id="theme-lib.trending-messages"/>
    <component id="kudos.widget.authors-leaderboard"/>
    <component id="forums.widget.unanswered-topics-taplet" mode="slim"/>
